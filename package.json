{
  "name": "irma-web-glue",
  "version": "0.1.3",
  "description": "Glue irma-web-frontend to irmajs",
  "main": "dist/irma-web-glue.js",
  "scripts": {
    "build": "webpack && npm run copy-styling && npm run update-example-version",
    "serve": "webpack-dev-server",
    "update-example-version": "echo \"{ \\\"version\\\": \\\"`git rev-parse HEAD`\\\" }\" > docs/version.json",
    "copy-styling": "cp -r node_modules/irma-web-frontend/dist/* docs/irma-web-frontend/"
  },
  "repository": {
    "type": "git",
    "url": "https://nuts-foundation.github.io/irma-web-glue/"
  },
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@privacybydesign/irmajs": "^0.1.8",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "irma-web-frontend": "^0.1.4",
    "qrcode": "^1.4.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {}
}
